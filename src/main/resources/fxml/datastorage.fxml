<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="GUI." :controller="sample.Controller"
>

    <GridPane fx:controller="sample.Controller"
              xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    </GridPane>


  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <TreeTableView layoutX="20.0" layoutY="52.0" prefHeight="328.0" prefWidth="600.0">
              <columns>
                <TreeTableColumn editable="false" prefWidth="200.0" text="Name" />
                <TreeTableColumn editable="false" minWidth="0.0" prefWidth="100.0" text="Format" />
                  <TreeTableColumn editable="false" prefWidth="150.0" text="Size" />
                  <TreeTableColumn editable="false" prefWidth="150.0" text="Date" />
              </columns>
            </TreeTableView>
            <ButtonBar layoutX="355.0" layoutY="14.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" prefWidth="265.0">
              <buttons>
                <Button mnemonicParsing="false" text="Download" />
                  <Button mnemonicParsing="false" text="Delete" />
                  <Button mnemonicParsing="false" text="Rename" />
              </buttons>
            </ButtonBar>
            <ButtonBar layoutX="20.0" layoutY="14.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0">
              <buttons>
                  <Button mnemonicParsing="false" text="Refresh" />
                  <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="120.0" text="Upload" />
              </buttons>
            </ButtonBar>
         </children>
    </AnchorPane>
  </children>
</VBox>
